{
  "name": "vite-pyide",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev-server":"cd server && poetry run python app/main.py -- --frontend \"..\\dist\" --webserverpath \"..\\.webide-server\"",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "generate-client": "openapi-ts --input http://localhost:21006/api/v1/openapi.json --output ./src/client --client axios",
    "install-all":"yarn install && cd server && poetry install",
    "build-all":"yarn build && cd server && poetry build"
  },
  "dependencies": {
    "@codemirror/lang-javascript": "^6.2.2",
    "@codemirror/lang-python": "^6.1.5",
    "@codemirror/theme-one-dark": "^6.1.2",
    "@icon-park/vue-next": "^1.4.2",
    "@radix-icons/vue": "^1.0.0",
    "@vueuse/components": "^10.9.0",
    "@vueuse/core": "^10.9.0",
    "@xterm/xterm": "^5.5.0",
    "axios": "^1.6.8",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "codemirror": "^6.0.1",
    "path-browserify": "^1.0.1",
    "pinia": "^2.1.7",
    "radix-vue": "^1.6.2",
    "reconnecting-websocket": "^4.4.0",
    "tailwind-merge": "^2.2.2",
    "tailwindcss-animate": "^1.0.7",
    "vue": "^3.4.21",
    "vue-codemirror": "^6.1.1",
    "vue-router": "^4.3.0",
    "xterm-theme": "^1.1.0"
  },
  "devDependencies": {
    "@hey-api/openapi-ts": "^0.42.1",
    "@iconify/vue": "^4.1.1",
    "@rushstack/eslint-patch": "^1.3.3",
    "@tsconfig/node20": "^20.1.2",
    "@types/node": "^20.12.7",
    "@types/path-browserify": "^1.0.2",
    "@vicons/ionicons5": "^0.12.0",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/tsconfig": "^0.5.1",
    "@xterm/addon-attach": "^0.11.0",
    "@xterm/addon-canvas": "^0.7.0",
    "@xterm/addon-fit": "^0.10.0",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.49.0",
    "eslint-plugin-vue": "^9.17.0",
    "naive-ui": "^2.38.1",
    "npm-run-all2": "^6.1.2",
    "postcss": "^8.4.38",
    "prettier": "^3.0.3",
    "tailwindcss": "^3.4.3",
    "typescript": "~5.4.0",
    "unplugin-auto-import": "^0.17.5",
    "unplugin-vue-components": "^0.26.0",
    "vfonts": "^0.0.3",
    "vite": "^5.1.6",
    "vite-plugin-vue-devtools": "^7.0.18",
    "vue-tsc": "^2.0.6"
  }
}
